{
  "name": "sequelize-treinamento",
  "version": "1.0.0",
  "description": "Este Ã© um projeto para fins de aprendizagem do SEQUELIZE, utilizando o NODEJS e EXPRESS.",
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.39.0",
    "sha256": "^0.2.0"
